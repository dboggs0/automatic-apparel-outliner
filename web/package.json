{
  "name": "automatic-apparel-outliner",
  "version": "0.0.1",
  "author": "Dave Boggs, Francisco Diaz, Sloan Lipman, Akshata Marathe, Andrew Neary",
  "homepage": "https://github.com/sloanlipman/automatic-apparel-outliner",
  "scripts": {
    "ng": "ng",
    "start": "ng build --outputHashing=all --watch",
    "build": "ng build --prod --aot --build-optimizer --vendor-chunk --output-hashing=all --named-chunks --extract-css",
    "pretty-print": "prettier --write '**/*.+(ts|scss|css|html|json)'",
    "pretty-list": "prettier --list-different '**/*.+(ts|scss|css|html|json)'",
    "test": "jest --collect-coverage --silent",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/cli": "~10.0.5",
    "@angular/common": "~10.0.0",
    "@angular/compiler-cli": "~10.0.0",
    "@angular/core": "~10.0.0",
    "@angular/forms": "~10.0.0",
    "@angular/platform-browser-dynamic": "~10.0.0",
    "@angular/platform-browser": "~10.0.0",
    "@angular/router": "~10.0.0",
    "@ionic-native/core": "^5.0.0",
    "@ionic-native/splash-screen": "^5.0.0",
    "@ionic-native/status-bar": "^5.0.0",
    "@ionic/angular": "^5.0.0",
    "rxjs": "~6.5.5",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.0",
    "@angular/compiler": "~10.0.0",
    "@angular/language-service": "~10.0.0",
    "@ionic/angular-toolkit": "^2.3.0",
    "@types/jest": "^26.0.10",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "husky": "^4.2.5",
    "jest": "^26.4.2",
    "jest-preset-angular": "^8.3.1",
    "prettier": "^2.1.1",
    "prettier-plugin-organize-imports": "^1.1.1",
    "pretty-quick": "^3.0.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.9.5"
  },
  "description": "An Ionic project"
}
